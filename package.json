{
  "name": "mSupplyColdChain",
  "version": "0.0.2",
  "description": "Cold chain temperature monitoring",
  "author": "mSupply Foundation",
  "main": "index.js",
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=12.8.4"
  },
  "private": true,
  "scripts": {
    "reset": "yarn start-no-options --reset-cache",
    "build": "node ./scripts/setup.js && cd ./android && ./gradlew assembleRelease",
    "start-no-options": "react-native run-android",
    "start": "node ./scripts/setup.js --mock-ble --redux-logger --query-logger --dev-logger && react-native run-android",
    "setup": "node ./scripts/setup.js",
    "madge": "node ./madge-tree",
    "make-debug-keystore": "keytool -genkey -v -keystore debug.keystore -storepass android -alias androiddebugkey -keypass android -keyalg RSA -keysize 2048 -validity 10000",
    "test": "jest",
    "storybook": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook",
    "prepare": "patch-package",
    "update-splash": "configure-splash-screen --statusbar-hidden -p \"android\" --resize-mode \"cover\" \"#282F46\" \"./assets/splash.png\"",
    "clean": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* &&  rm -rf node_modules/ && npm cache verify && npm install && npm start --reset-cache"
  },
